    "def add_mul(choice, *args):\n",
    "    if choice == \"add\": #매개변수 choice에 \"add\"을 입력받았을 때\n",
    "        result = 0\n",
    "        for i in args:\n",
    "            result = result + i\n",
    "    elif choice ==\"mul\": #매개변수 choice에 \"mul\"을 입력받았을 때\n",
    "        result = 1\n",
    "        for i in args:\n",
    "            result = result *i\n",
    "    elif choice ==\"sub\": #매개변수 choice에 \"sub\"을 입력받았을 때\n",
    "        result = 0\n",
    "        for i in args:\n",
    "            result = result -i \n",
    "    elif choice ==\"div\": # 매개변수 choice에 \"div\"을 입력받았을 때\n",
    "        result = 1\n",
    "        for i in args:\n",
    "            result = result /i\n",
    "    else:\n",
    "        result = choice\n",
    "        print(\"정확하게 입력하세요.\")\n",
    "        \n",
    "    return result\n"
   ]
